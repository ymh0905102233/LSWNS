        app:layout_constraintRight_toLeftOf="@+id/textView175"
        android:layout_marginRight="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView172"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView175"
